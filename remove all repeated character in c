#include<stdio.h>
 #include<string.h>
 int main()
 {
     char s[]="kirankumar";
     int l=strlen(s),c=0,i=0,j=0,n=0;
     for(i=0;i<l;i++)
     {
         for(j=0;j<i;j++)
         {
             if(s[i]==s[j])
                 break;
         }
         if(i==j)
         {
             printf("%c",s[i]);
         }
     }
 }
