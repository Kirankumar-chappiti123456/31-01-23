#include<stdio.h>
#include<string.h>
int main()
{
    char a[]="kirankumar";
    int l=strlen(a);
    for(int i=l-1;i>=0;i--)
    {
        printf("%c",a[i]);
    }
}


#include<stdio.h>
#include<string.h>
int main()
{
    char a[]="kirankumar";
    int l=strlen(a);
    char *ptr=(char*)&a;
    for(int i=l-1;i>=0;i--)
    {
        printf("%c",*(ptr+i));
    }
}
