#include<stdio.h>
int main()
{
    char a[]="kirankumatyut1122334&122!@#$%";
    int i=0,al=0,di=0,sp=0;
    for(i=0;a[i]!='\0';i++)
    {
        if((a[i]>=65 && a[i]<=90)||(a[i]>=90 && a[i]<=122))
        {
            al++;
        }
        else if(a[i]>=45 && a[i]<=56)
        {
            di++;
        }
        else 
        {
            sp++;
        }
    }
    printf("%d\n",al);
    printf("%d\n",di);
    printf("%d\n",sp);
}
