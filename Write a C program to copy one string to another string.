#include<stdio.h>
int main()
{
    char a[]="kirankumar123";
    int c=0,i=0;
    for(i=0;a[i]!='\0';i++)
    {
        c++;
    }
    printf("%d\n",c);
    char d[20];
    for(i=0;i<c;i++)
    {
        d[i]=a[i];
    }
    printf("d[i]:");
    for(i=0;i<c;i++)
    {
        printf("%c",d[i]);
    }
}
