#include<stdio.h>
 #include<string.h>
 int main()
 {
     char s[]="kirankumar",ch='k';
     int l=strlen(s),c=0,i=0,j=0,n=0;
     for(i=0;i<l;i++)
     {
         if(s[i]==ch)
         {
         for(j=i;j<l;j++)
         {
             s[j]=s[j+1];
         }
         l--;
         i--;
         }
     }
     printf("the remove element:%c=%s",ch,s);
 }
 
 
